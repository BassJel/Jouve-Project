/* Spell checker 
----------------------------------------------- */
.spellCheck {
}

.spellCheck:after {
	/*Add margin between menu and rest of content in Firefox*/
	content: ".";
	display: block;
	height: 0;
	clear: both;
	visibility: hidden;
}

.spellCheck ul {
}

.spellCheck ul li {
	display: inline;
	padding: -5;
	margin: 0;
	color:#004682;
}

.spellCheck .question {
	color: #666;
	font-size:110%;
	font-weight: bold;
	margin: 0 0 5px 0;
}

.spellCheck ul .suggestion {
	cursor: pointer;
	text-decoration: underline;
	color:#004682;
}

.spellCheck ul .suggestion .motSuggere {
	text-align: left;
	text-decoration: underline;
	font-weight: bold;
}

.spellCheck ul li a {
	font-weight: bold;
	color:#004682;
	/*padding: 4px 7px;*/
	text-decoration: none;
	
}

.spellCheck ul li a:hover,.spellCheck ul li a.selected {
	/*script dynamically adds a class of "selected" to the current active menu item*/
	
}

/* ######### Style for Drop Down Menu ######### */
.dropmenudiv {
	position:fixed;
	top: 0;
	border: 1px solid #BBB; /*THEME CHANGE HERE*/
	border-bottom-width: 0;
	font:normal 12px Verdana;
	line-height:18px;
	z-index:100;
	background-color: white;
	width: 200px;
	visibility: hidden;
}

.dropmenudiv a {
	width: auto;
	display: block;
	text-indent: 3px;
	border-bottom: 1px solid #BBB; /*THEME CHANGE HERE*/
	padding: 2px 0;
	text-decoration: none;
	font-weight: bold;
	color:#004682;
}

* html .dropmenudiv a{ /*IE only hack*/
