<wicket:panel>
	<a wicket:id="transfertLink">Transf√©rer des documents...</a>
	<div wicket:id="transfertPanel">
		<p style="color: red" wicket:id="errorMessageLabel"></p>
		<table cellpadding="5" cellspacing="5"><tr>
			<td><input type="checkbox" wicket:id="allNoneCheckbox" style="margin-left: 26px;"/></td>
			<td wicket:id="transfertButtons">
				<a wicket:id="launchTransfertLink">
					<span wicket:id="launchTransfertLabel"></span>
				</a>
			</td>
		</tr></table>
	</div>
	<div wicket:id="searchResults">
		<table cellpadding="0" cellspacing="0">
			<tr>
				<wicket:enclosure child="icon">
				<td style="vertical-align:top;">
					<img wicket:id="icon" src="" alt="" style="margin: 20px 0 0 0;" />
				</td>
				<td>
					&nbsp;
				</td>
				</wicket:enclosure>
				<td style="vertical-align:top;">
					<table cellpadding="2" cellspacing="2">
						<tr><td style="vertical-align: top;"><div style="margin-top: 20px;"><input type="checkbox" wicket:id="checkedResults"/></div></td><td><div wicket:id="searchResultPanel" class="detailResult"></div></td></tr>
						<tr><td></td><td><div wicket:id="taggingPanel"></div></td></tr>
					</table>	
	       		</td>
	       	</tr>
	    </table>	
	</div>
	<div id="pagination">
        <span wicket:id="resultsPN"></span>
    </div>
    <div wicket:id="noResultsMessage" style="text-align: center">
    <br/>
    	<wicket:message key="noResults">[noResults]</wicket:message>
    </div>
</wicket:panel>

</html>