<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xmlns:wicket="http://wicket.sourceforge.net" xml:lang="cs" lang="cs">
<wicket:panel>
<wicket:extend>

	<table class="formulaire">
		<tr>
			<td class="etiquette">
				<wicket:message key="name">[Name]</wicket:message> :
			</td>
			<td>
				<input wicket:id="name" type="text" /> <wicket:message key="unmodifiable">[(Non modifiable)]</wicket:message> 
			</td>
		</tr>
		<tr>
			<td class="etiquette">
				<wicket:message key="locales">[Locales]</wicket:message> :
			</td>
			<td>
				<div wicket:id="localesCheckGroup">
				<div wicket:id="locales">
					<input wicket:id="localeCheck" type="checkbox" />
					<span wicket:id="localeName">[Locale Name]</span>
				</div>
				</div>
			</td>
		</tr>
		<tr>
			<td class="etiquette">
				<wicket:message key="titleField">[Title]</wicket:message> :
			</td>
			<td wicket:id="titleContainer">
				<div wicket:id="title">
					<input wicket:id="titleField" type="text" />
					<span wicket:id="localeName">[Locale Name]</span>
				</div>
			</td>
		</tr>
		<tr>
			<td class="etiquette">
				<wicket:message key="description">[Description]</wicket:message> :
			</td>
			<td wicket:id="descriptionContainer">
				<div wicket:id="description">
					<textarea wicket:id="descriptionLocale" cols="20" rows="10"></textarea>
					<span wicket:id="localeName">[Locale Name]</span>
				</div>
			</td>
		</tr>
		<tr>
			<td class="etiquette">
				<wicket:message key="publicCollection">[Public]</wicket:message> :
			</td>
			<td>
				<input wicket:id="publicCollection" type="checkbox" />
			</td>
		</tr>
		<tr>
			<td class="etiquette">
				<wicket:message key="openSearchURL">[OpenSearch URL]</wicket:message> :
			</td>
			<td>
				<input wicket:id="openSearchURL" type="text" />
			</td>
		</tr>
	</table>
	
</wicket:extend>
</wicket:panel>
</html>