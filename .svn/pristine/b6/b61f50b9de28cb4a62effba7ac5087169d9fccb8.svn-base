<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns:wicket="http://wicket.sourceforge.net">
<wicket:panel>
<form wicket:id="form">
<table class="formulaire">
	<tr>
		<td class="etiquette">
			<wicket:message key="languageInSearchForm"></wicket:message>
		</td>
		<td>
			<input wicket:id="languageInSearchForm" type="checkbox"/>
			<br/>
			<font color="red">* <wicket:message key="needDocLanguageFacet">[needDocLanguageFacet]</wicket:message></font>
		</td>
	</tr>
	<tr>
		<td class="etiquette">
			<wicket:message key="currentSearchFacet"></wicket:message>
		</td>
		<td>
			<input wicket:id="currentSearchFacet" type="checkbox"/>
		</td>
	</tr>
	<tr>
		<td class="etiquette">
			<wicket:message key="descriptionAsExcerpt"></wicket:message>
		</td>
		<td>
			<input wicket:id="descriptionAsExcerpt" type="checkbox"/>
		</td>
	</tr>
	<tr>
		<td class="etiquette">
			<wicket:message key="showCollectionsInResultFacets"></wicket:message>
		</td>
		<td>
			<input wicket:id="showCollectionsInResultFacets" type="checkbox"/>
		</td>
	</tr>
	<tr>
		<td class="etiquette">
			<wicket:message key="resultsInNewWindow"></wicket:message>
		</td>
		<td>
			<input wicket:id="resultsInNewWindow" type="checkbox"/>
		</td>
	</tr>
	<tr>
		<td class="etiquette">
			<wicket:message key="alwaysDisplayTags"></wicket:message>
		</td>
		<td>
			<input wicket:id="alwaysDisplayTags" type="checkbox"/>
		</td>
	</tr>
	<tr>
		<td class="etiquette">
			<wicket:message key="translateLanguageNames"></wicket:message>
		</td>
		<td>
			<input wicket:id="translateLanguageNames" type="checkbox"/>
		</td>
	</tr>
	<tr>
		<td class="etiquette">
			<wicket:message key="advancedSearchEnabled"></wicket:message>
		</td>
		<td>
			<input wicket:id="advancedSearchEnabled" type="checkbox"/>
		</td>
	</tr>
	<tr>
		<td class="etiquette">
			<wicket:message key="truncateDisplayUrlAfter">[Truncate URLs after...]</wicket:message>
		</td>
		<td>
			<input wicket:id="truncateDisplayUrlAfter" type="text" size="4" />	
			(<wicket:message key="emptyDoNotTruncate">[Empty=Do not truncate]</wicket:message>)	
		</td>
	</tr>
	<tr>
		<td colspan="2" align="center">
			<input type="submit" wicket:message="value:save" class="btnRech" />
		</td>
	</tr>
</table>
</form>
		
</wicket:panel>
</html>